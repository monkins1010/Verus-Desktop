{
  "name": "verus-desktop",
  "productName": "Verus-Desktop",
  "version": "0.9.0-4",
  "description": "Verus Desktop Wallet App",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "debug": "yarn start devmode",
    "make-patch": "./make-patch.sh",
    "pack": "electron-builder --dir verus-unpacked",
    "dist": "electron-builder --publish=never  ",
    "dist-win": "electron-builder --publish=never  --win",
    "test": "mocha",
    "update-agamalib": "rm -rf node_modules/agama-wallet-lib/ && yarn install",
    "postinstall": "rm -rf node_modules/bitgo-utxo-lib/node_modules/create-hash"
  },
  "engines": {
    "node": ">11"
  },
  "repository": "https://github.com/VerusCoin/Verus-Desktop/",
  "homepage": "https://veruscoin.io",
  "keywords": [
    "agama",
    "SuperNET",
    "komodo",
    "multicoin",
    "wallet",
    "spv",
    "jumblr",
    "verus"
  ],
  "author": {
    "name": "Verus",
    "email": "development@veruscoin.io",
    "url": "https://veruscoin.io"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "1.6.10",
    "cross-env": "5.2.1",
    "electron": "16.0.4",
    "electron-builder": "22.9.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.11.0",
    "tslint": "6.1.3",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@electron/remote": "1.1.0",
    "@ethersproject/signing-key": "5.0.5",
    "agama-wallet-lib": "git+https://github.com/VerusCoin/agama-wallet-lib.git#bd1ff58f25de7c041ee8670d669637b4e72a4e77",
    "arch": "2.1.0",
    "async": "2.6.0",
    "axios": "0.22.0",
    "base64url": "https://github.com/michaeltout/base64url.git",
    "bigi": "1.4.2",
    "bitgo-utxo-lib": "git+https://github.com/VerusCoin/bitgo-utxo-lib.git",
    "blake2b": "https://github.com/VerusCoin/blake2b.git",
    "bluebird": "3.4.7",
    "body-parser": "1.18.2",
    "buffer-reverse": "1.0.1",
    "coinselect": "github:bitcoinjs/coinselect",
    "create-hash": "https://github.com/pbca26/createHash#electron-v4",
    "crypto-js": "4.0.0",
    "csv-writer": "1.6.0",
    "ethers": "5.1.0",
    "ethers-michaeltout": "github:michaeltout/ethersjs-package",
    "express": "4.16.0",
    "fix-path": "2.1.0",
    "fs-extra": "9.1.0",
    "graceful-fs": "4.1.11",
    "iocane": "1.0.0",
    "node-cache": "5.1.2",
    "nodejs-aes256": "1.0.1",
    "passwd-strength": "https://github.com/michaeltout/passwd-strength.git",
    "portscanner": "2.1.1",
    "remote-file-size": "3.0.5",
    "semver-compare": "1.0.0",
    "sha256": "0.2.0",
    "socket.io": "3.1.1",
    "systeminformation": "5.6.11",
    "unzipper": "0.10.11",
    "verus-typescript-primitives": "https://github.com/VerusCoin/verus-typescript-primitives.git",
    "verus-wallet-endpoints": "https://github.com/michaeltout/verus-wallet-endpoints.git",
    "wif": "2.0.6",
    "xmldom": "0.6.0"
  },
  "optionalDependencies": {
    "osx-temperature-sensor": "1.0.4"
  },
  "build": {
    "appId": "Verus",
    "files": [
      "**/*",
      "!buildscripts",
      "!dist",
      "!test${/*}",
      "!windeps${/*}",
      "!.git*",
      "!Brewfile",
      "!README.md",
      "!make*",
      "!check_submodule.sh",
      "!gui/EasyDEX/react/node_modules${/*}",
      "!gui/EasyDEX/react/src${/*}",
      "!gui/EasyDEX/react/www${/*}"
    ],
    "includePdb": false,
    "asar": false,
    "mac": {
      "artifactName": "${productName}-v${version}.${ext}",
      "icon": "assets/icons/agama_icons/vrsc.icns",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "icon": "assets/icons/vrsc.icns",
      "contents": [
        {
          "x": 135,
          "y": 220
        },
        {
          "x": 405,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "artifactName": "${productName}-v${version}-${arch}.${ext}",
      "icon": "assets/icons/agama_icons",
      "desktop": {
        "Encoding": "UTF-8"
      },
      "target": [
        "appimage"
      ]
    },
    "win": {
      "artifactName": "${productName}-v${version}.${ext}",
      "target": [
        "nsis"
      ],
      "icon": "assets/icons/vrsc.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE",
      "installerIcon": "assets/icons/vrsc.ico"
    }
  },
  "resolutions": {
    "lodash": "4.17.21",
    "js-yaml": "3.13.1",
    "tar": "4.4.18",
    "https-proxy-agent": "2.2.4",
    "mem": "4.0.0",
    "bin-links": "1.1.6",
    "yargs-parser": "13.1.2",
    "minimist": "1.2.5",
    "elliptic": "6.5.4",
    "bn.js": "4.11.8",
    "dot-prop": "5.2.0",
    "npm-registry-fetch": "4.0.5",
    "ini": "1.3.8",
    "ssri": "8.0.1",
    "y18n": "5.0.5",
    "hosted-git-info": "3.0.8",
    "ajv": "6.12.3",
    "ws": "7.4.6",
    "normalize-url": "4.5.1",
    "path-parse": "1.0.7",
    "ansi-regex": "5.0.1",
    "qs": "6.2.3",
    "debug": "2.6.9",
    "fresh": "0.5.2",
    "mime": "1.4.1",
    "json-schema": "0.4.0"
  }
}
